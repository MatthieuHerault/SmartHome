<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/launch_background_2"
    app:layout_constraintStart_toEndOf="parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/img_launch_icon"
        android:layout_width="140dp"
        android:layout_height="150dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="500dp"
        app:srcCompat="@drawable/launch_icon" />

    <Button
        android:id="@+id/btn_salon_sud"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_salon_sud"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_salon_sud"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="100dp"
        android:layout_marginEnd="250dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_salon_ouest"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_salon_ouest"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_salon_ouest"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="100dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_cuisine"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_cuisine"
        android:paddingLeft="8dp"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_cuisine"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="100dp"
        android:layout_marginStart="250dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_chambre_1"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_chambre"
        android:paddingLeft="8dp"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_chambre_1"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="250dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_chambre_2"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_chambre"
        android:paddingLeft="8dp"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_chambre_2"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="180dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_chambre_3"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_chambre"
        android:paddingLeft="8dp"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_chambre_3"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="180dp"
        android:layout_marginStart="250dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_tout_ouvrir"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_tout_ouvrir"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_tout_ouvrir"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="460dp"
        android:layout_marginEnd="250dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_tout_fermer"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_tout_fermer"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_tout_fermer"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="460dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

    <Button
        android:id="@+id/btn_camera"
        android:layout_width="110dp"
        android:layout_height="130dp"
        android:background="@drawable/bordure"
        android:drawableTop="@drawable/ic_camera"
        android:fontFamily="@font/marvel_bold"
        android:text="@string/btn_camera"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="460dp"
        android:layout_marginStart="250dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>